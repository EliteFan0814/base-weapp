.header-wrap .header {
  position: fixed;
  width: 750rpx;
}

.header-wrap .header .route-wrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 17px;
  line-height: 32px;
  font-weight: bold;
  text-align: center;
  color: #000;
}

.header-wrap .header .route-wrap .item {
  box-sizing: border-box;
  width: 33.33%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.header-wrap .header .route-wrap .left {
  padding-left: 7px;
  justify-content: flex-start;
}

.content {
  border: 1px solid red;
  background-color: #f5f5f5;
  height: 2000rpx;
}
